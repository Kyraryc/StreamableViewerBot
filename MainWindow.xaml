<Window x:Name="Kyraryc_s_Improvised_Bot" x:Class="StreamableViewerBot.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:StreamableViewerBot"
        mc:Ignorable="d"
        Title="Kyraryc's Improvised Bot" Height="450" Width="778.571">
    <Grid Margin="0,0,154,-21">
        <Button x:Name="lblStatus" Content="Begin!" HorizontalAlignment="Left" Height="133" Margin="206,97,0,0" VerticalAlignment="Top" Width="282" RenderTransformOrigin="0.5,0.5" AutomationProperties.Name="btnGo" Click="Button_Click" FontSize="36">
            <Button.RenderTransform>
                <TransformGroup>
                    <ScaleTransform/>
                    <SkewTransform AngleX="-0.114"/>
                    <RotateTransform Angle="0.461"/>
                    <TranslateTransform X="0.135" Y="0.001"/>
                </TransformGroup>
            </Button.RenderTransform>
        </Button>
        <TextBox x:Name="txtStatus" HorizontalAlignment="Left" Height="48" Margin="128,299,0,0" TextWrapping="Wrap" Text="Ready to go" VerticalAlignment="Top" Width="476" FontSize="22" AutomationProperties.Name="txtStatus" IsEnabled="False" TextDecorations="{x:Null}"/>

    </Grid>
</Window>
